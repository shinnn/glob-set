{
	"name": "glob-set",
	"version": "1.0.1",
	"description": "Like ‘glob’ module, but provides matched paths as a Set instead of an Array",
	"repository": "shinnn/glob-set",
	"author": "Shinnosuke Watanabe (https://github.com/shinnn)",
	"scripts": {
		"pretest": "eslint .",
		"test": "nyc node test.js"
	},
	"license": "MIT",
	"files": [
		"index.js"
	],
	"keywords": [
		"glob",
		"match",
		"matching",
		"pattern",
		"wildcard",
		"filter",
		"file",
		"directory",
		"search",
		"scan",
		"find",
		"set",
		"promise",
		"async"
	],
	"dependencies": {
		"glob": "^7.1.3",
		"glob-option-error": "^1.0.0",
		"inspect-with-kind": "^1.0.5",
		"validate-glob-opts": "^1.0.2"
	},
	"devDependencies": {
		"@shinnn/eslint-config-node": "^6.0.0",
		"eslint": "^5.4.0",
		"nyc": "^13.0.1",
		"nyc-config-common": "^1.0.0",
		"tape": "^4.9.1",
		"unglobbable": "^1.0.1"
	},
	"eslintConfig": {
		"extends": "@shinnn/node",
		"overrides": [
			{
				"files": [
					"index.js"
				],
				"rules": {
					"promise/prefer-await-to-then": "off"
				}
			}
		]
	},
	"nyc": {
		"extends": "nyc-config-common"
	}
}
